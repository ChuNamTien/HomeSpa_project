<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Booking.
    -->
    <changeSet id="20210531022546-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="bookings_id"
                                 baseTableName="booking_treatment"
                                 constraintName="fk_booking_treatment_bookings_id"
                                 referencedColumnNames="id"
                                 referencedTableName="booking"/>
        <addForeignKeyConstraint baseColumnNames="treatments_id"
                                 baseTableName="booking_treatment"
                                 constraintName="fk_booking_treatment_treatments_id"
                                 referencedColumnNames="id"
                                 referencedTableName="treatment"/>
        
        <addForeignKeyConstraint baseColumnNames="customer_id"
                                 baseTableName="booking"
                                 constraintName="fk_booking_customer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="customer"/>

    </changeSet>
</databaseChangeLog>
